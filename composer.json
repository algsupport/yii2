    "support": {
        "issues": "https://github.com/algsupport/yii2/issues?state=open",
        "forum": "https://forum.yiiframework.com/",
        "wiki": "https://www.yiiframework.com/wiki",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "source": "https://github.com/algsupport/yii2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "algsupport/yii2": "self.version"
    },
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*",
        "algsupport/yii2-composer": "~2.0.4",
        "ezyang/htmlpurifier": "^4.6",
        "cebe/markdown": "~1.0.0 | ~1.1.0 | ~1.2.0",
        "bower-asset/jquery": "^3.7.0",
        "bower-asset/inputmask": "^5.0.8 ",
        "bower-asset/punycode": "^2.2",
        "paragonie/random_compat": ">=1"
    },
    "require-dev": {
        "cebe/indent": "~1.0.2",
        "friendsofphp/php-cs-fixer": "~2.2.3 | ^3.0",
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
    "autoload": {
        "psr-4": {
            "yii\\": "framework/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "bin": [
        "framework/yii"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
